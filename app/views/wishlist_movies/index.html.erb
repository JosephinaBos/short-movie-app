<h1>Welcome to your wishlist page</h1>
<p>This is your wishlist</p>



<ul>
<% @wishlist.each do |wishlist| %>
  <li><%= wishlist.movie.name %> (<%= wishlist.movie.summary %>)</li>
  <% end %>
</ul>

  <div class="btn-delete">
    <div class="btn btn-danger">
      <%= link_to 'Delete movie from your wishlist', movie_wishlist_movie_path(params[:movie_id]), method: :delete %>
    </div>
  </div>

