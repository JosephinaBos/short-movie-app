<h1>Welcome to your wishlist page</h1>
<p>This is your wishlist</p>



<ul>
<% @wishlist.each do |wishlist| %>
  <li><%= wishlist.movie.name %> (<%= wishlist.movie.summary %>)
      <div class="btn-delete">
    <div class="btn btn-danger">
      <%= link_to 'Delete movie from your wishlist', movie_wishlist_movie_path(wishlist, wishlist.movie), method: :delete %>
    </div>
  </div>
  </li>

  <% end %>
</ul>



